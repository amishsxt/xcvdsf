#include<iostream>
#include<string>
using namespace std;

class Employee
{
public:

int Id,Salary;
string Name,Company_name;

Employee(int Id1,string Name1,int Salary1,string Company_name1)
{
Id=Id1;
Name=Name1;
Salary=Salary1;
Company_name=Company_name1;
}

void setId(int Id1)
{
Id=Id1;
}

void setName(string Name1)
{
Name=Name1;
}

void setSalary(int Salary1)
{
Salary=Salary1;
}

void setCompany_name(string Company_name1)
{
Company_name=Company_name1;
}

int getId()
{
return Id;
}

string getName()
{
return Name;
}

int getSalary()
{
return Salary;
}

string getCompany_name()
{
return Company_name;
}

};

int main()
{

Employee e1(1,"Ashu",50000,"abc");
e1.setId(10);
e1.setName("Amish");
e1.setSalary(90000);
e1.setCompany_name("Google");

cout << e1.getId() << "\n";
cout << e1.getName() << "\n";
cout << e1.getSalary() << "\n";
cout << e1.getCompany_name() << "\n";

}



